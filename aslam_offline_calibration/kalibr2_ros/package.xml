<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>kalibr2_ros</name>
  <version>0.0.1</version>

  <description>Utilities to interface ROS2 with Kalibr2.</description>

  <maintainer email="francisco.rossi.ctr@tri.global">Francisco Rossi</maintainer>

  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>aslam_time</depend>
  <depend>kalibr2</depend>
  <depend>rclcpp</depend>
  <depend>rosbag2_storage</depend>
  <depend>rosbag2_transport</depend>
  <depend>rosbag2_cpp</depend>
  <depend>sensor_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>tf2_msgs</depend>
  <depend>cv_bridge</depend>
  <depend>yaml-cpp</depend>
  <depend>rosidl_runtime_cpp</depend>

  <test_depend>ament_cmake_gtest</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
